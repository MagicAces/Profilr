{
  "name": "profilr",
  "version": "1.0.0",
  "description": "A Student's data collection system",
  "main": "app.ts",
  "scripts": {
    "test": "ts-node backend/app.ts",
    "start": "ts-node backend/app.ts",
    "server": "nodemon backend/app.ts",
    "client": "npm run dev --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm install && npm install --prefix frontend && npm run build --prefix frontend"
  },
  "prisma": {
    "schema": "backend/prisma/schema.prisma",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} backend/prisma/seed.ts"
  },
  "keywords": [
    "Images",
    "Students",
    "Data"
  ],
  "author": "\"Isaac\"",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.15.1",
    "axios": "^1.7.2",
    "body-parser": "^1.20.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "express": "^4.19.2",
    "express-async-handler": "^1.2.0",
    "express-session": "^1.18.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/connect-flash": "^0.0.40",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.5",
    "@types/node": "^20.14.5",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.16",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
